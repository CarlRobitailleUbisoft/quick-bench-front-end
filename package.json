    "cross-spawn": "6.0.4",
    "css-loader": "0.28.9",
    "detect-port": "1.2.2",
    "dotenv": "5.0.0",
    "eslint": "4.17.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "filesize": "3.6.0",
